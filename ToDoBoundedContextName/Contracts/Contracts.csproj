<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<!-- Any 3.1+ target provides nullable annotations -->
		<TargetFrameworks>net7.0;netstandard2.0</TargetFrameworks>
		<AssemblyName>__ToDoAreaName__.__ToDoBoundedContextName__.Contracts</AssemblyName>
		<RootNamespace>__ToDoAreaName__.__ToDoBoundedContextName__.Contracts</RootNamespace>
		<Nullable>Enable</Nullable>
		<ImplicitUsings>Enable</ImplicitUsings>
		<LangVersion>11</LangVersion>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<Version>0.1.0</Version>
	</PropertyGroup>
	
	<ItemGroup>
		<InternalsVisibleTo Include="__ToDoAreaName__.__ToDoBoundedContextName__.Application" />
	</ItemGroup>

</Project>
